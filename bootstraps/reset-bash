echo "bash-it disable alias all"
bash-it disable alias all
echo "bash-it disable plugin all"
bash-it disable plugin all
echo "bash-it disable completion all"
bash-it disable completion all

## [alias]
bash-it enable alias general

## [plugin]
bash-it enable plugin base alias-completion

## [completion]
bash-it enable completion bash-it system

echo '[bash_it reset done]'
