#!/usr/bin/env bash

set -o errexit
set -o nounset
set -o pipefail
(shopt -p inherit_errexit &>/dev/null) && shopt -s inherit_errexit

readonly SCRIPT_DIR="$(cd -P -- "$(dirname -- "$0")" && pwd -P)"

plugins=( $(cat "$SCRIPT_DIR"/../../bootstraps/recommends/custom_plugins ) )

a plugins-enable "${plugins[@]}"
